@startuml TRIGGERS_Diagram

entity "TRIGGERS" as TRIGGERS {
  _id : objectId
  name : string
  description : string
  scope : string
  triggerTag : string
  assets : list
  type : string
  startDate : int
  triggerComponents : list
  createdBy : objectId
  triggerSensorId : objectId
  status : string
  isOpen : boolean
  isDeleted : boolean
  createdAt : object
  updatedAt : object
