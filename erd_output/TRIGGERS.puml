}

entity "CREATED AT" as CREATED_AT {
}

entity "UPDATED AT" as UPDATED_AT {
}

' // Relationships
TRIGGERS ||--|| CREATED_AT : has_createdAt
TRIGGERS ||--|| UPDATED_AT : has_updatedAt
@enduml